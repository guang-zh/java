public class RockPaperScissors{
  public static void main(String[] args){
   java.util.Scanner reader=new java.util.Scanner(System.in);
   System.out.println("Choose from: rock, paper, or scissors");
   System.out.println("Player 1 choose: ");
   String player1=reader.next();
   System.out.println("Player 2 choose: ");
   String player2=reader.next();
   
   if (player1.equals(player2)){
    System.out.println("The game is tie!");
   }
   else if(player1.equals("rock")){
      if (player2.equals("scissors")){
      System.out.println("Player1 with rock wins");
      }
      else{
      System.out.println ("Player2 with paper wins"); 
       }
   else if (player1.equals("scissors")){
      if (player2.equals("rock")){
      System.out.println("Player2 with rock wins");
      }
      else {
      System.out.println ("Player1 with paper wins"); 
      }      
    else {
      if (player2.equals("rock")){
      System.out.println("Player1 with paper wins");
      }
          else {
           System.out.println ("Player2 with scissors wins"); 
          }     
    }
    
   }

